<h3 class="ui header">{{t "forms.i-i-s-test11-заявка-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номЗаяв
      class=(if (v-get validationModel "номЗаяв" "isInvalid") "error")
      label=(t "forms.i-i-s-test11-заявка-e.номЗаяв-caption")
      data-test-i-i-s-test11-заявка-e-номЗаяв=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номЗаяв" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.важност
      class=(if (v-get validationModel "важност" "isInvalid") "error")
      label=(t "forms.i-i-s-test11-заявка-e.важност-caption")
      data-test-i-i-s-test11-заявка-e-важност=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "важност" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.состоян
      class=(if (v-get validationModel "состоян" "isInvalid") "error")
      label=(t "forms.i-i-s-test11-заявка-e.состоян-caption")
      data-test-i-i-s-test11-заявка-e-состоян=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "состоян" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.текст
      class=(if (v-get validationModel "текст" "isInvalid") "error")
      label=(t "forms.i-i-s-test11-заявка-e.текст-caption")
      data-test-i-i-s-test11-заявка-e-текст=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "текст" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-test11-заявка-e.контрагент-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.контрагент
      relationName="контрагент"
      projection="КонтрагентL"
      title=(t "forms.i-i-s-test11-заявка-e.контрагент-caption")
      readonly=readonly
      componentName="контрагентLookup"
      class=(if (v-get validationModel "контрагент" "isInvalid") "error")
      data-test-i-i-s-test11-заявка-e-контрагент=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "контрагент" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-test11-заявка-e.сотрудник-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сотрудник
      relationName="сотрудник"
      projection="СотрудникL"
      title=(t "forms.i-i-s-test11-заявка-e.сотрудник-caption")
      readonly=readonly
      componentName="сотрудникLookup"
      class=(if (v-get validationModel "сотрудник" "isInvalid") "error")
      data-test-i-i-s-test11-заявка-e-сотрудник=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сотрудник" "message")}}
  </div>
</form>
